https://codeforces.com/group/MWSDmqGsZm/contest/219856/problem/V
----------------------------------------------------------------------------
#include<bits/stdc++.h>
using namespace std;

int main() {
    string s;
    cin >> s;

    int index=s.find("EGYPT");
    while ((index = s.find("EGYPT")) != string::npos) {
        s.erase(index, 5);          
        s.insert(index, " ");       
    }

    cout << s << endl;

    return 0;
}

---------------------------------------------------------------------
#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    cin>>s;
    while(1)
    {
        int index=s.find("EGYPT");

    if(s.find("EGYPT")!=-1)
    {
        s.erase(index,5);
        s.insert(index," ");
    }
    else
    {
        break;
    }
    }
    
    cout<<s<<endl;
    return 0;
}
