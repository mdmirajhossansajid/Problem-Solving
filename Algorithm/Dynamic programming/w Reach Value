#include<bits/stdc++.h>
using namespace std;
int dp[100000];
bool rec(long long int n)
{
    if(n==1)
        return true;
    else if(n==0)
      return false;
    if(dp[n]!=-1)
        return dp[n];
    bool l=false;
    bool r=false;
    if(n%10==0)
     l=rec(n/10);
    if(n%20==0)
        r=rec(n/20);
    dp[n]=l || r;
    return dp[n];
    
}
int main()
{   memset(dp,-1,sizeof(dp));
    int t;cin>>t;
    while(t--)
    {
        int a;cin>>a;
        if(rec(a))
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;
    }
    return 0;
}
