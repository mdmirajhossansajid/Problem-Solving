#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,x;cin>>n>>x;
    vector<int>arr(n);
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int cnt=0;
    set<int>s(arr.begin(),arr.end());
    for (int i = 0; i < x; i++)
    {
        if(s.find(i)==s.end())
            cnt++;
    }
    if(s.find(x)!=s.end())
        cnt++;
    cout<<cnt<<endl;
    return 0;
}
