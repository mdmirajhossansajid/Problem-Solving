#include<bits/stdc++.h>
using namespace std;
int main()
{   int ans1=0,ans2=0,mx;
    int who = 1;
    int n;cin>>n;
    deque<int>dq(n);
    for(int i=0;i<n;i++)
        cin>>dq[i];
    while(!dq.empty())
    {
        int lv=dq.front(),rv=dq.back();
        mx=max(lv,rv);
        
        if(who%2!=0)
            ans1+=mx;
        else
            ans2+=mx;
        if(mx==lv)
            dq.pop_front();
        else
            dq.pop_back();

        who++;
    }
    cout<<ans1<<" "<<ans2<<endl;
    return 0;
}
