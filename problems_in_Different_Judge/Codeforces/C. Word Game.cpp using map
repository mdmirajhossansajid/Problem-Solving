#include<bits/stdc++.h>
using namespace std;
int main()
{   int t;cin>>t;
    while(t--)
    {
        int n,m=3;
        cin>>n;
        map<string,vector<int>>mp;
        for(int i=0;i<m;i++)
        {
            for(int j=0;j<n;j++)
            {
                string s;cin>>s;
                mp[s].push_back(i);
            }
        }
        vector<int>v(m+1);
        for (auto it = mp.begin(); it != mp.end(); it++)
        {
            vector<int> rounds = it->second; 
            if (rounds.size() == 1)
            {
                v[rounds[0]] += 3;
            }
            else if (rounds.size() == 2)
            {
                v[rounds[0]] += 1;
                v[rounds[1]] += 1;
            }
        }
        
        for (int i = 0; i < m; i++)
        {
            cout << v[i] << " ";
        }
        cout << "\n";
    }
    
    return 0;
}
